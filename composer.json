{
    "name": "konekt/appshell",
    "description": "Foundation for Laravel business applications",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": ["laravel", "concord", "concord-box"],
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/artkonekt/appshell"
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "konekt/concord": "^1.11",
        "konekt/customer": "^2.3.1",
        "konekt/address": "^2.2",
        "konekt/user": "^2.4",
        "konekt/acl": "^1.6",
        "konekt/menu": "^1.9",
        "konekt/gears": "^1.8",
        "konekt/enum": "^3.0|^4.0",
        "konekt/enum-eloquent": "^1.8.1",
        "laravelcollective/html": "^6.3",
        "laracasts/flash": "^3.2.1",
        "cviebrock/eloquent-sluggable": "^9.0|^10.0",
        "diglactic/laravel-breadcrumbs": "^8.0",
        "laravel/framework": "^9.2||!=9.15.0|^10.0"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0|^8.0",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
          "Konekt\\AppShell\\": "src"
        },
        "files": [
            "src/Support/helpers.php",
            "src/Support/theme.php",
            "src/Support/icon.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\AppShell\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
